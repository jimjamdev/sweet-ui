{
  "name": "@jimjamz/sweet-ui",
  "version": "1.0.1",
  "description": "Styled-components based UI library",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "npm run release",
    "release": "npm run styleguide:build && npm run build:dev && npm run build:prod",
    "start": "nodemon -w src -x npm run build:dev && npm run styleguide",
    "build:dev": "NODE_ENV=development rollup -c",
    "build:prod": "npm run styleguide:build && NODE_ENV=production rollup -c",
    "styleguide": "styleguidist server --open",
    "styleguide:build": "styleguidist build",
    "clean": "rimraf -r ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimmyjamieson/sweet-ui.git"
  },
  "author": "Ian Jamieson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jimmyjamieson/sweet-ui/issues"
  },
  "homepage": "https://jimmyjamieson.github.io/sweet-ui/",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-babel-module": "^5.0.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.5.0",
    "nodemon": "^1.18.10",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-styleguidist": "^9.0.4",
    "rimraf": "^2.6.3",
    "rollup": "^1.6.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-replace": "^2.1.0",
    "webpack": "^4.29.6"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "polished": "^3.0.3",
    "prop-types": "^15.7.2",
    "styled-components": "^4.1.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "directories": {
    "doc": "docs"
  },
  "keywords": [
    "styled-components",
    "ui"
  ]
}
