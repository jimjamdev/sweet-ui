{
  "presets": [
    ["@babel/preset-env", { "modules": false }],
    "@babel/preset-react"
  ],
  "plugins": [
    "lodash",
    ["styled-components", { "ssr": true }],
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "@atoms": "./atoms",
        "@molecules": "./molecules",
        "@organisms": "./organisms",
        "@theme": "./theme",
        "@utils": "./utils"
      }
    }]
  ]
}
